{
  "manifest_version": 2,
  "name": "Wasp Extension",
  "description": "Chrome extension and would find each element that contanin text from list of strings.",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
      "16":"icons/alpha_64.png",
      "48":"icons/alpha_64.png",
      "128":"icons/alpha_64.png"    
    },
    "default_popup": "popup.html"
  },
  "icons":{
    "16":"icons/alpha_64.png",
    "48":"icons/alpha_64.png",
    "128":"icons/alpha_64.png"    
  },
  "permissions": ["tabs", "contextMenus", "notifications", "alarms", "storage"],
  "content_scripts":[
	  {
		  "matches": ["<all_urls>"],
		  "js": ["js/content.js"],
      "css": ["css/content.css"]
	  }
  ]	


}
